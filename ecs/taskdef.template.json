{
  "family": "testpj",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "__ECS_TASK_EXECUTION_ROLE_ARN__",
  "taskRoleArn": "__ECS_TASK_ROLE_ARN__",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "__IMAGE_URI__",
      "essential": true,
      "portMappings": [{ "containerPort": 8080, "protocol": "tcp" }],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/my-spring-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
